#include <stdio.h>
int main()
{
	int a = 0;
	int b = 0;
	printf("请输入两个整数");
	scanf("5%d%d", &a, &b);
	if (a >= b)
		printf("%d",a);
	else
		printf("%d",b);
	return 0;
}

较简约的方式
#include <stdio.h>
int main()
{
	int a = 0;
	int b = 0;
	scanf("%d%d", &a, &b);
	a >= b ? printf("%d",a) : printf("%d",b);
	return 0;
}

