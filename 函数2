//用函数法打印100到200之间的素数
#include <stdio.h>
 hello(int x)
{
	int b = 0;
	for (b = 2; b < x; b++)
	{
		if (x % b == 0)
			return 0;
	}
	return 1;
}
int main()
{
	int a = 0;
	for (a = 100; a <= 200; a++)
	{
		if (hello(a) == 1)
			printf("%d\n", a);
	}
	return 0;
}


//用函数判断闰年
#include <stdio.h>

hahaa(int x)
{
	if ((x%4==0&&x%100!=0)||x%400==0)
		return 1;
	else
		return 0;
}
int main()
{
	int a = 0;
	scanf("%d", &a);
	if (hahaa(a) == 1)
		printf("是");
	else
		printf("不是");
	return 0;
}
